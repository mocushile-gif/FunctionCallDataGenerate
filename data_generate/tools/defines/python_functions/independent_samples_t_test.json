    "parameters": {
      "type": "object",
      "properties": {
        "sample1": {
          "description": "The first sample of observations.",
          "type": "array",
          "items": {
            "type": "number"
          }
        },
        "sample2": {
          "description": "The second sample of observations.",
          "type": "array",
          "items": {
            "type": "number"
          }
        },
        "alpha": {
          "description": "The significance level of the test. Defaults to 0.05.",
          "type": "number",
          "default": 0.05
        }
      },
      "required": ["sample1", "sample2"]
    }
  }
  
            },
            "list_date_min": {
                "type": "string",
                "description": "Minimum listing date (formatted as 'YYYY-MM-DD')."
            },
            "open_house_max": {
                "type": "string",
                "description": "Maximum open house date (formatted as 'YYYY-MM-DD')."
            },
            "has_tour": {
                "type": "boolean",
                "description": "Whether the property has a virtual tour."

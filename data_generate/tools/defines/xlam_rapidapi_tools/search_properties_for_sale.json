            },
            "list_date_max": {
                "type": "string",
                "description": "Maximum listing date (formatted as 'YYYY-MM-DD')."
            },
            "list_price_max": {
                "type": "number",
                "description": "Maximum listing price."
            },
            "baths_min": {
                "type": "number",

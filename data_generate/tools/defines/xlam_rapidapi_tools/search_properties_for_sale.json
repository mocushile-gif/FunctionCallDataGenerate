            "lot_sqft_max": {
                "type": "number",
                "description": "Maximum lot square footage."
            },
            "hoa_fee_optional_min": {
                "type": "number",
                "description": "Minimum optional HOA fee."
            },
            "sqft_max": {
                "type": "number",
                "description": "Maximum lot square footage."
            },
            "sqft_min": {
                "type": "number",
                "description": "Minimum lot square footage."

{
    "name": "compress_pdf",
    "description": "Compress a PDF file to reduce file size while maintaining quality.",
    "parameters": {
        "type": "object",
        "properties": {
            "input_path": {
                "type": "string",
                "description": "Path to input PDF file"
            },
            "output_path": {
                "type": "string",
                "description": "Path for output file (optional, auto-generated if not provided)"
            },
            "compression_level": {
                "type": "string",
                "description": "Compression level",
                "enum": ["low", "medium", "high", "extreme"],
                "default": "medium"
            },
            "image_quality": {
                "type": "integer",
                "description": "Image quality for compression (1-100)",
                "default": 85,
                "minimum": 1,
                "maximum": 100
            },
            "remove_metadata": {
                "type": "boolean",
                "description": "Whether to remove metadata",
                "default": false
            }
        },
        "required": ["input_path"]
    }
}